

main: def: {
  desc: *'' | string
}

main: sdk: target: &: {
  name: .$KEY
  active: *true | boolean
}


main: sdk: entity: &: {
  name: .$KEY
  active: *true | boolean
}


main: sdk: feature: &: {
  name: .$KEY
  active: *false | boolean
  title: string
  hook: &: {
    active: *false | boolean
  }

  hook: PreConstruct: {}
  hook: PostConstruct: {}
}


main: sdk: option: &: {
  name: .$KEY
  active: *true | boolean
}





